msgid "Success!"
msgstr "成功！"

#: ../kano_login/login.py:246
msgid "You're in - online features now enabled."
msgstr "入れた - オンライン機能が使用可能"

#: ../kano_login/login.py:292
msgid "Reset your password"
msgstr "パスワードリセット"

#: ../kano_login/login.py:293
msgid "We'll send a new password to your email"
msgstr "新しいパスワードをメールで送ります"

#: ../kano_login/login.py:298
msgid "Email"
msgstr "メール"

#: ../kano_login/login.py:315
msgid "Reset password"
msgstr "パスワードをリセットする"

#: ../kano_login/login.py:340
msgid "Sent new password to your email"
msgstr "新しいパスワードをメールで送りました"

#: ../kano_login/login.py:342
msgid "Go to login screen"
msgstr "ログイン画面へ"

#: ../kano_login/login.py:343 ../kano_login/login.py:349
msgid "Quit"
msgstr "終了"

#: ../kano_login/login.py:346
msgid "Something went wrong!"
msgstr "失敗しました！"

#: ../kano_login/login.py:350
msgid "Try again"
msgstr "やり直す"

